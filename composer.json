{
    "name": "silverstripe/versioned-snapshot-admin",
    "description": "SilverStripe Versioned Snapshot Admin",
    "type": "silverstripe-vendormodule",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "versioned",
        "snapshots"
    ],
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "http://silverstripe.com"
        },
        {
            "name": "The SilverStripe Community",
            "homepage": "http://silverstripe.org"
        }
    ],
    "require": {
        "php": "^8.3",
        "silverstripe/graphql": "^5",
        "silverstripe/versioned-snapshots": "^2",
        "silverstripe/versioned-admin": "^2"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SnapshotAdmin\\": "src/",
            "SilverStripe\\SnapshotAdmin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev",
            "dev-3": "3.x-dev",
            "dev-2": "2.x-dev",
            "dev-1": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
